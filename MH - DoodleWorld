local oldtick = tick()
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({
    Name = "Manta Hub - Doodle World",
    HidePremium = false,
    IntroText = "MantaHub",
   --IntroIcon = "rbxassetid://13169412462"
})
On12345 = 0

local AllPlayers = game:GetService("Players")
local SelfPlayer = AllPlayers.LocalPlayer
local me = tostring(SelfPlayer)

-- functions

function doodleheal()
    local args = {
        [1] = "Heal",
        [2] = "Printer"
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
end

function collect()
    local args = {
        [1] = "CollectTickets",
        [2] = "1"
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataFunc"):InvokeServer(unpack(args))
end  

function isbusy()
    local args = {
        [1] = "ToggleBusy",
        [2] = true
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
    
end

function notbusy()
    local args = {
        [1] = "ToggleBusy",
        [2] = false
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
    
end

function bike()
    local args = {
        [1] = "MountBike",
        [2] = true
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
end    

function nobike()
    local args = {
        [1] = "MountBike",
        [2] = false
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
end   

--functions activated on start


-- Tabs:
    local Main = Window:MakeTab({
        Name = "Main"
    })
    local Encounters = Window:MakeTab({
        Name = "Encountering"
    })
    local Transport = Window:MakeTab({
        Name = "Transportation"
    })


    Main:AddButton({
        Name = "Heal",
        Callback = function()
            doodleheal()
        end
    })
    Main:AddButton({
        Name = "Collect Tickets",
        Callback = function()
            collect()
        end
    })
    Main:AddButton({
        Name = "Enable Remote PC",
        Callback = function()
            loadstring(game:HttpGet(('https://raw.githubusercontent.com/Aidez/doodle_world/main/storage_access_button'),true))()
        end
    })
    Main:AddToggle({
        Name = "Busy",
        Default = false,
        Callback = function(Value)
            while Value==true do
            wait(1)
            isbusy()
            end
        end    
    })
    Transport:AddToggle({
        Name = "Ride Bike (Indoors)",
        Default = false,
        Callback = function(bikevalue)
        if bikevalue == true then
                bike()
            
        else 
            nobike()
        end
                
            end
        
    })

