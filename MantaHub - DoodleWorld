local oldtick = tick()
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({
    Name = "Manta Hub - Doodle World",
    HidePremium = false,
    IntroText = "MantaHub",
   --IntroIcon = "rbxassetid://13169412462"
})
On12345 = 0

local AllPlayers = game:GetService("Players")
local SelfPlayer = AllPlayers.LocalPlayer
local me = tostring(SelfPlayer)




-- functions

function doodleheal()
    local args = {
        [1] = "Heal",
        [2] = "Printer"
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
end

local Noclip = nil
local Clip = nil

function noclip()
	Clip = false
	local function Nocl()
		if Clip == false and game.Players.LocalPlayer.Character ~= nil then
			for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
				if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
					v.CanCollide = false
				end
			end
		end
		wait(0.21) -- basic optimization
	end
	Noclip = game:GetService('RunService').Stepped:Connect(Nocl)
end

function clip()
	if Noclip then Noclip:Disconnect() end
	Clip = true
end



function collect()
    local args = {
        [1] = "CollectTickets",
        [2] = "1"
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataFunc"):InvokeServer(unpack(args))
    local args = {
        [1] = "CollectTickets",
        [2] = "2"
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataFunc"):InvokeServer(unpack(args))

end  

function isbusy()
    local args = {
        [1] = "ToggleBusy",
        [2] = true
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
    
end

function notbusy()
    local args = {
        [1] = "ToggleBusy",
        [2] = false
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
    
end

function bike()
    local args = {
        [1] = "MountBike",
        [2] = true
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
end    

function nobike()
    local args = {
        [1] = "MountBike",
        [2] = false
    }
    
    game:GetService("Players"):WaitForChild(me):WaitForChild("Remotes"):WaitForChild("PlayerDataEvent"):FireServer(unpack(args))
end   

--functions activated on start


-- Tabs:
    local Main = Window:MakeTab({
        Name = "Main"
    })
    local Encounters = Window:MakeTab({
        Name = "Encountering"
    })
    local Transport = Window:MakeTab({
        Name = "Movement"
    })
    local Stats= Window:MakeTab({
        Name = "Statistics"
    })

   
   
    local money = Stats:AddLabel("Money: Awaiting Refresh")
    local vp = Stats:AddLabel("VP: Awaiting Refresh")
    local tickets = Stats:AddParagraph("Tickets:","Awaiting Refresh")

    Stats:AddButton({
        Name="Refresh",
        Callback = function()
            local args = {
                [1] = "GetMoney"
            }
            Money = game:GetService("Players"):WaitForChild("Nebula_Manta"):WaitForChild("Remotes"):WaitForChild("PlayerDataFunc"):InvokeServer(unpack(args))
            money:Set("Money:"..Money)

            local args = {
                [1] = "GetVP"
            }
            VP = game:GetService("Players"):WaitForChild("Nebula_Manta"):WaitForChild("Remotes"):WaitForChild("PlayerDataFunc"):InvokeServer(unpack(args))
            vp:Set("VP:"..VP)

            local args = {
                [1] = "GetTickets"
            }
            
            Tickets = game:GetService("Players"):WaitForChild("Nebula_Manta"):WaitForChild("Remotes"):WaitForChild("PlayerDataFunc"):InvokeServer(unpack(args))
            whole = ""
            for i, v in pairs(Tickets) do
                current = i..":"..v
                whole = whole.."\n"..current
                tickets:Set(whole)
                
           end
           print(whole)
           
        end
        
        
    })
    
    

    Main:AddButton({
        Name = "Heal",
        Callback = function()
            doodleheal()
        end
    })
    Main:AddButton({
        Name = "Collect Tickets",
        Callback = function()
            collect()
        end
    })
    Main:AddButton({
        Name = "Enable Remote PC",
        Callback = function()
            loadstring(game:HttpGet(('https://raw.githubusercontent.com/Aidez/doodle_world/main/storage_access_button'),true))()
        end
    }) 
    Transport:AddToggle({
        Name = "Ride Bike (Indoors)",
        Default = false,
        Callback = function(bikevalue)
        if bikevalue == true then
                bike()
            
        else 
            nobike()
        end
                
            end
        
    })
    Transport:AddToggle({
        Name = "Noclip",
        Default = false,
        Callback = function(noclipping)
        if noclipping == true then
                noclip()
            
        else 
            clip()
        end
                
            end
        
    })
    Transport:AddSlider({
        Name = "Character Speed",
        Min=0,
        Max=500,
        Default=16,
        Increment=10,
        Color = Color3.fromRGB(100,100,100),
        ValueName="Studs/Second",
        Callback = function(speed)
            print(speed)
            getgenv().WalkSpeedValue = speed;
            local Player = game:service'Players'.LocalPlayer;
            Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
            Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
            end)
            Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;

        end
        
    })
    

